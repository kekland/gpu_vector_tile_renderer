  "version": "0.2.0",
  "configurations": [
    {
      "name": "maplibre_flutter_gpu",
      "request": "launch",
      "type": "dart",
      "args": [
        "--enable-impeller",
        "--enable-experiment=native-assets"
      ]
    },
    {
      "name": "maplibre_flutter_gpu (profile mode)",
      "request": "launch",
      "type": "dart",
      "flutterMode": "profile"
    },
    {
      "name": "maplibre_flutter_gpu (release mode)",
      "request": "launch",
      "type": "dart",
      "flutterMode": "release"
    }
  ]
}