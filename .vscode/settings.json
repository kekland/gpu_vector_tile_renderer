{
  "dart.flutterSdkPath": ".fvm/versions/3.27.1",
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/Thumbs.db": true,
    ".fvm": true,
    "**/.idea": true,
    "**/*.iml": true,
    ".metadata": true,
    "build": true,
    "pubspec.lock": true,
    ".fvmrc": true
  },
  "editor.rulers": [
    120
  ],
  "files.associations": {
    "*.arb": "json",
    "unordered_map": "cpp",
    "new": "cpp"
  },
  "dart.lineLength": 120,
}